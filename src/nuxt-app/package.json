{
  "name": "nuxt3-net6-starter",
  "private": true,
  "packageManager": "pnpm@7.5.0",
  "scripts": {
    "build": "nuxt build",
    "dev:nossl": "nuxt dev --port 5002",
    "dev": "node aspnetcore-https && run-script-os",
    "dev:windows": "nuxt dev --port 5002 --https --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "dev:default": "nuxt dev --port 5002 --https --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "generate": "nuxt generate",
    "preview": "nuxt preview"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "date-fns": "^2.23.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@formkit/nuxt": "1.0.0-beta.10",
    "@iconify-json/carbon": "^1.1.6",
    "@iconify-json/twemoji": "^1.1.5",
    "@jcamp/tailwindcss-plugin-icons": "0.1.0",
    "@nuxtjs/color-mode": "^3.1.4",
    "@nuxtjs/tailwindcss": "^5.3.1",
    "@pinia/nuxt": "^0.3.0",
    "@sfxcode/nuxt-primevue": "^0.6.0",
    "@sfxcode/formkit-primevue": "^0.6.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/lodash-es": "^4.17.6",
    "@unocss/nuxt": "^0.44.5",
    "@vueuse/nuxt": "^8.9.4",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "3.0.0-rc.6",
    "pinia": "^2.0.16",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "primevue": "^3.15.0",
    "primeicons": "^5.0.0",
    "sass": "^1.54.0",
    "run-script-os": "^1.1.6",
    "typescript": "^4.7.4",
    "tailwindcss": "^3.1.7"
  }
